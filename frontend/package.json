{
  "name": "traffic-guardian",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "framer-motion": "^12.15.0",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "lucide-react": "^0.522.0",
    "postcss": "^8.4.31",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hls-player": "^3.0.7",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.6.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.8.1",
    "svgo": "^3.3.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=150",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "stylelint": "stylelint 'src/**/*.css' --max-warnings=0",
    "stylelint:fix": "stylelint 'src/**/*.css' --fix",
    "lint:all": "npm run lint && npm run stylelint",
    "lint:all:fix": "npm run lint:fix && npm run stylelint:fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,json,md}'",
    "type-check": "tsc --noEmit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "e2e": "cypress run --headless",
    "e2e:open": "cypress open",
    "validate": "npm run lint:all && npm run type-check && npm run test:ci",
    "pre-commit": "npm run format && npm run lint:all:fix && npm run type-check && npm run test:coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cypress": "^1.1.6",
    "@types/leaflet": "^1.9.18",
    "@types/leaflet.heat": "^0.2.5",
    "@types/node": "^16.18.126",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@types/socket.io-client": "^1.4.36",
    "core-js-pure": "^3.42.0",
    "cypress": "^14.4.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^25.7.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^2.8.8",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^25.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts"
    ]
  },
  "overrides": {
    "nth-check": ">=2.0.1",
    "webpack-dev-server": ">=4.0.0",
    "@svgr/plugin-svgo": {
      "nth-check": ">=2.0.1"
    }
  }
}
